#include <iostream>
using namespace std;

int countValid(int* arr[], int size) {
    int cnt = 0;
    int** p = arr;
    for (int i = 0; i < size; i++, p++) {
        if (*p != nullptr) cnt++;
    }
    return cnt;
}

int main() {
    int n;
    cout << "Enter size: ";
    cin >> n;

    int** arr = new int*[n];
    cout << "Enter elements (number or - for null): ";
    for (int i = 0; i < n; i++) {
        string s;
        cin >> s;
        if (s == "-") arr[i] = nullptr;
        else arr[i] = new int(stoi(s));
    }

    cout << "Count of valid pointers: " << countValid(arr, n);
}
